{
  "name": "react-redux-toolkit-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:json": "concurrently \"npm run start\" \"npm run server:json\"",
    "server:json": "json-server --watch json_server/db.json --port 3004",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --debug src/",
    "lint:check": "eslint src/ && prettier --check src/",
    "lint:fix": "eslint --debug src/ --fix && prettier --write src/",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write src/"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.3",
    "@mui/material": "5.14.3",
    "@mui/x-data-grid": "6.11.0",
    "@reduxjs/toolkit": "1.9.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.27",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-redux": "7.1.25",
    "@types/react-router-dom": "5.3.3",
    "bootstrap": "5.2.3",
    "bootstrap-icons": "1.10.5",
    "material-react-table": "1.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "react-virtuoso": "4.4.2",
    "sass": "1.62.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.8",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "babel-eslint": "10.1.0",
    "concurrently": "8.0.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "json-server": "0.17.3",
    "prettier": "2.8.8"
  }
}
